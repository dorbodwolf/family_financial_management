<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">单笔收入详情</h3>
  </div>
  <div class="panel-body">
    <ul class="list-group">
      <li class="list-group-item">收入类型：<%= @income.income_type %></li>
      <li class="list-group-item">收入细类：<%= @income.subtype %></li>
      <li class="list-group-item">入账额：<%= @income.value %></li>
      <li class="list-group-item">入账日：<%= @income.credit_day %></li>
      <li class="list-group-item">细则：<%= @income.detail %></li>
      <li class="list-group-item">录入时间：<%= @income.created_at %></li>
      <li class="list-group-item">更新时间：<%= @income.updated_at %></li>
    </ul>
  </div>
  <div class="panel-footer"><%= @income.created_at %></div>
</div>
<%= render "incomes/shared/links" %>
